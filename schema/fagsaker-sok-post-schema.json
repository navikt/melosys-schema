{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://melosys.nav.no/schemas/sok-fagsaker-post-schema.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "ident",
    "saksnummer"
  ],
  "properties": {
    "ident": {
      "$id": "#/items/properties/ident",
      "type": [
        "string",
        "null"
      ],
      "title": "The Ident Schema",
      "default": "",
      "examples": [
        "01013011111"
      ],
      "pattern": "^(.*)$"
    },
    "saksnummer": {
      "$id": "#/items/properties/saksnummer",
      "type": [
        "string",
        "null"
      ],
      "title": "The Saksnummer Schema",
      "default": "",
      "examples": [
        "MEL-4"
      ],
      "pattern": "^(.*)$"
    }
  }
}

