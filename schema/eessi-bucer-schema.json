{
  "definitions": {
    "bucID": {
      "type": "string",
      "examples": ["40000","40001"]
    },
    "bucType": {
      "type": "string",
      "examples": ["LA_BUC_01"]
    },
    "sedID": {
      "type": "string",
      "examples": ["e2312fdda0b8464fdabdabfaecfb25c9"]
    },
    "sedType": {
      "type": "string",
      "default": "",
      "examples": ["A002","A005"],
      "pattern": "^(.*)$"
    },
    "status": {
      "type": "string",
      "enum": ["MOTTATT", "UTKAST", "AVBRUTT", "SENDT"]
    }
  },
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://melosys.nav.no/schemas/eessi-bucer-schema.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "bucer"
  ],
  "properties": {
    "bucer": {
      "$id": "#/properties/bucer",
      "type": "array",
      "title": "The Bucer Schema",
      "items": {
        "$id": "#/properties/bucer/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "bucID",
          "bucType",
          "opprettetDato",
          "seder"
        ],
        "properties": {
          "bucID": {
            "$id": "#/properties/bucer/items/properties/bucID",
            "$ref": "#/definitions/bucID"
          },
          "bucType": {
            "$id": "#/properties/bucer/items/properties/bucType",
            "$ref": "#/definitions/bucType"
          },
          "opprettetDato": {
            "$id": "#/properties/bucer/items/properties/opprettetDato",
            "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/dato"
          },
          "seder": {
            "$id": "#/properties/bucer/items/properties/seder",
            "type": "array",
            "title": "The Seder Schema",
            "items": {
              "$id": "#/properties/bucer/items/properties/seder/items",
              "type": "object",
              "title": "The Items Schema",
              "required": [
                "bucID",
                "sedID",
                "sedType",
                "status",
                "opprettetDato",
                "sistOppdatert",
                "rinaUrl"
              ],
              "properties": {
                "bucID": {
                  "$id": "#/properties/bucer/items/properties/seder/items/properties/bucID",
                  "$ref": "#/definitions/bucID"
                },
                "sedID": {
                  "$id": "#/properties/bucer/items/properties/seder/items/properties/sedID",
                  "$ref": "#/definitions/sedID"
                },
                "sedType": {
                  "$id": "#/properties/bucer/items/properties/seder/items/properties/sedType",
                  "$ref": "#/definitions/sedType"
                },
                "status": {
                  "$id": "#/properties/bucer/items/properties/seder/items/properties/status",
                  "$ref": "#/definitions/status"
                },
                "opprettetDato": {
                  "$id": "#/properties/bucer/items/properties/seder/items/properties/opprettetDato",
                  "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/dato"
                },
                "sistOppdatert": {
                  "$id": "#/properties/bucer/items/properties/seder/items/properties/sistOppdatert",
                  "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/dato"
                },
                "rinaUrl": {
                  "$id": "#/properties/bucer/items/properties/seder/items/properties/rinaUrl",
                  "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/url"
                }
              }
            }
          }
        }
      }
    }
  }
}
