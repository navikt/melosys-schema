{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://melosys.nav.no/schemas/definitions-schema.json",
  "title": "The Definitions Schema",
  "definitions": {
    "nullable-aktoerID": {
      "type": ["string", "null"]
    },
    "aarmaaned": {
      "type": "string",
      "pattern": "^\\d{4}-\\d{2}$",
      "examples": [
        "2016-12"
      ]
    },
    "adresse": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "adressetype"
      ],
      "oneOf": [
        {
          "properties": {
            "adressetype": {"const": "STRUKTURERT"}
          },
          "required": ["strukturertAdresse"]
        },
        {
          "properties": {
            "adressetype": {"const": "USTRUKTURERT"}
          },
          "required": ["ustrukturertAdresse"]
        }
      ],
      "properties": {
        "adressetype": {
          "type": "string",
          "enum": ["STRUKTURERT", "USTRUKTURERT"]
        },
        "strukturertAdresse": {
          "$ref": "#/definitions/strukturertAdresse"
        },
        "ustrukturertAdresse": {
          "$ref": "#/definitions/ustrukturertAdresse"
        }
      }
    },
    "behandlingID": {
      "type": "integer",
      "title": "The BehandlingID Schema",
      "default": 0,
      "examples": [
        1000023948
      ]
    },
    "behandlingstype": {
      "type": "string",
      "title": "The Behandlingstype schema",
      "examples": ["SOEKNAD"],
      "enum": [
        "SOEKNAD",
        "KLAGE",
        "ANKE",
        "REGISTRERING_UNNTAK_NORSK_TRYGD_UTSTASJONERING",
        "REGISTRERING_UNNTAK_NORSK_TRYGD_ØVRIGE",
        "UTL_MYND_UTPEKT_NORGE",
        "UTL_MYND_UTPEKT_SEG_SELV",
        "NY_VURDERING",
        "ENDRET_PERIODE",
        "ANMODNING_OM_UNNTAK_HOVEDREGEL",
        "ØVRIGE_SED",
        "VURDER_TRYGDETID"
      ]
    },
    "behandlingsresultatTypeKode": {
      "type": "string",
      "enum": [
        "FASTSATT_LOVVALGSLAND",
        "FORELOEPIG_FASTSATT_LOVVALGSLAND",
        "HENLEGGELSE",
        "ANMODNING_OM_UNNTAK",
        "IKKE_FASTSATT",
        "REGISTRERT_UNNTAK",
        "AVSLAG_MANGLENDE_OPPL"
      ]
    },
    "databaseID": {
      "type": "integer",
      "title": "The DatabaseID Schema",
      "examples": [
        1234
      ]
    },
    "nullable-databaseID": {
      "type": ["integer", "null"],
      "title": "The Nullable-databaseID Schema",
      "examples": [
        1234,
        null
      ]
    },
    "dato":  {
      "type": "string",
      "format": "date",
      "examples": ["2018-08-27"]
    },
    "nullable-dato":  {
      "type": ["string","null"],
      "format": "date",
      "examples": ["2018-08-27"]
    },
    "dato-tid": {
      "type": "string",
      "format": "date-time",
      "examples": [
        "2018-08-27T07:16:03.273Z"
      ]
    },
    "nullable-dato-tid": {
      "type": ["string","null"],
      "format": "date-time",
      "examples": [
        "2018-08-27T07:16:03.273Z", null
      ]
    },
    "dokument":  {
      "type": "object",
      "title": "Dokument type definisjon",
      "additionalProperties": false,
      "required": [
        "dokumentID",
        "tittel"
      ],
      "properties": {
        "dokumentID": {"$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/dokumentID"
        },
        "tittel": {
          "type": ["string", "null"],
          "title": "Tittel Schema",
          "examples": ["Tittelen her"]
        }
      }
    },
    "nullable-dokument":  {
      "type": "object",
      "title": "Nullable Dokument type definisjon",
      "additionalProperties": false,
      "required": [
        "dokumentID",
        "tittel"
      ],
      "properties": {
        "dokumentID": {
          "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/nullable-dokumentID"
        },
        "tittel": {
          "type": "string",
          "title": "Tittel Schema",
          "examples": ["Tittelen her"]
        }
      }
    },
    "dokumentID": {
      "$id": "#/properties/dokumentID",
      "type": ["string"],
      "title": "The DokumentID Schema",
      "default": "",
      "examples": [
        "DOK_ID_4"
      ]
    },
    "nullable-dokumentID": {
      "$id": "#/properties/dokumentID",
      "type": ["string","null"],
      "title": "The Nullable DokumentID Schema",
      "default": "",
      "examples": [
        "DOK_ID_4", null
      ]
    },
    "fnr": {
      "type": "string",
      "default": "",
      "examples": [
        "81007251212"
      ],
      "pattern": "^\\d{11}$"
    },
    "nullable-fnr": {
      "type": ["string", "null"],
      "default": "",
      "examples": [
        "81007251212"
      ],
      "pattern": "^\\d{11}$|^$"
    },
    "journalpostID": {
      "$id": "#/properties/journalpostID",
      "type": "string",
      "title": "The JournalpostID Schema",
      "default": "",
      "examples": [
        "DOK_321"
      ]
    },
    "kodeverk": {
      "type": "object",
      "title": "The Kodeverk Schema",
      "additionalProperties": false,
      "required": [
        "kode",
        "term"
      ],
      "properties": {
        "kode": {
          "type": "string",
          "title": "The Kode Schema",
          "default": "",
          "examples": [
            "OPPRETTET"
          ],
          "pattern": "^(.*)$"
        },
        "term": {
          "type": "string",
          "title": "The Term Schema",
          "default": "",
          "examples": [
            "Opprettet"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "landkode": {
      "type": "string",
      "minLength": 2,
      "examples": [
        "NO"
      ],
      "pattern": "^(.*)$"
    },
    "nullable-landkode": {
      "type": ["string", "null"],
      "minLength": 2,
      "examples": [
        "NO"
      ],
      "pattern": "^(.*)$"
    },
    "landkoder": {
      "type": "array",
      "additionalItems": false,
      "items": {
        "$ref": "#/definitions/landkode"
      }
    },
    "orgnr": {
      "type": "string",
      "default": "123456789",
      "examples": [
        "810072512"
      ],
      "pattern": "^\\d{9}$"
    },
    "nullable-orgnr": {
      "type": ["string", "null"],
      "default": "",
      "examples": [
        "810072512"
      ],
      "pattern": "\\d{9}$|^$"
    },
    "oppgaveID": {
      "title": "The OppgaveID schema",
      "type": "string",
      "default": "",
      "examples": [
        "177057928"
      ],
      "pattern": "^(.*)$"
    },
    "oppgavetype": {
      "title": "The Oppgavetype schema",
      "type": ["string", "null"],
      "examples": ["BEH_SAK_MK", "JFR"],
      "enum": [
        "BEH_SAK_MK",
        "JFR",
        null
      ]
    },
    "periodeID": {
      "type": "integer",
      "title": "The PeriodeID Schema",
      "default": 0,
      "examples": [
        400110740
      ]
    },
    "periode": {
      "title": "Periode med fom tom",
      "type": "object",
      "required": [
        "fom",
        "tom"
      ],
      "additionalProperties": false,
      "properties": {
        "fom": {
          "type": ["string"],
          "format": "date",
          "examples": ["2014-03-12"]
        },
        "tom": {
          "type": ["string","null"],
          "format": "date",
          "examples": ["2014-03-12", null]
        }
      }
    },
    "nullable-periode": {
      "title": "Periode med fom tom",
      "type": "object",
      "required": [
        "fom",
        "tom"
      ],
      "additionalProperties": false,
      "properties": {
        "fom": {
          "type": ["string", "null"],
          "format": "date",
          "examples": ["2014-03-12"]
        },
        "tom": {
          "type": ["string","null"],
          "format": "date",
          "examples": ["2014-03-12", null]
        }
      }
    },
    "enum-representerer": {
      "type": "string",
      "enum": ["BRUKER", "ARBEIDSGIVER", "BEGGE"]
    },
    "representererKode": {
      "title": "The Representerer Schema",
      "default": null,
      "examples": ["BRUKER", "ARBEIDSGIVER", "BEGGE", null],
      "oneOf": [
        { "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/enum-representerer"},
        { "type": ["null"] }
      ]
    },
    "gateadresse": {
      "title": "The Gateadresse Schema",
      "$id": "#/definitions/gateadresse",
      "type": "object",
      "additionalProperties": false,
      "required": [
        "gatenummer",
        "husbokstav",
        "husnummer",
        "gatenavn"
      ],
      "properties": {
        "gatenummer": {
          "$id": "#/definitions/gateadresse/properties/gatenummer",
          "type": [
            "integer",
            "null"
          ],
          "title": "The Gatenummer Schema",
          "default": null,
          "examples": [
            null
          ]
        },
        "husbokstav": {
          "$id": "#/definitions/gateadresse/properties/husbokstav",
          "type": [
            "string",
            "null"
          ],
          "title": "The Husbokstav Schema",
          "default": null,
          "examples": [
            null
          ]
        },
        "husnummer": {
          "$id": "#/definitions/gateadresse/properties/husnummer",
          "type": [
            "integer",
            "null"
          ],
          "title": "The Husnummer Schema",
          "default": null,
          "examples": [
            null
          ]
        },
        "gatenavn": {
          "$id": "#/definitions/gateadresse/properties/gatenavn",
          "type": [
            "string",
            "null"
          ],
          "title": "The Gatenavn Schema",
          "default": "",
          "examples": [
            "Olav Kyrres gate 9"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "registerAdresse": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "gateadresse",
        "postnr",
        "poststed",
        "land"
      ],
      "properties": {
        "gateadresse": {
          "title": "The Gateadresse Schema",
          "$id": "#/definitions/registerAdresse/properties/gateadresse",
          "$ref": "#/definitions/gateadresse"
        },
        "postnr": {
          "$id": "#/definitions/registerAdresse/properties/postnr",
          "type": [
            "string",
            "null"
          ],
          "title": "The Postnr Schema",
          "default": "",
          "examples": [
            "3936"
          ],
          "pattern": "^(.*)$"
        },
        "poststed": {
          "$id": "#/definitions/registerAdresse/properties/poststed",
          "type": [
            "string",
            "null"
          ],
          "title": "The Poststed Schema",
          "default": "",
          "examples": [
            "PORSGRUNN"
          ],
          "pattern": "^(.*)$"
        },
        "land": {
          "$id": "#/definitions/registerAdresse/properties/land",
          "type": [
            "string",
            "object",
            "null"
          ],
          "title": "The Land Schema",
          "default": "",
          "examples": [
            "NORGE"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "saksnummer": {
      "type": "string",
      "title": "The Saksnummer Schema",
      "examples": ["MEL-1"],
      "minLength": 1,
      "pattern": "^\\S+$"
    },
    "sakstype": {
      "type": "string",
      "title": "The Sakstype Schema",
      "examples": [
        "EU_EOS",
        "Trygdeavtale",
        "FTLR"
      ],
      "enum": [
        "EU_EOS",
        "TRYGDEAVTALE",
        "FTRL"
      ]
    },
    "strukturertAdresse": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "gatenavn",
        "husnummer",
        "region",
        "postnummer",
        "poststed",
        "landkode"
      ],
      "properties": {
        "gatenavn": {
          "type": "string",
          "minLength": 1,
          "examples": [
            "Waldemar Thranes gate"
          ]
        },
        "husnummer": {
          "type": ["string", "null"],
          "minLength": 1,
          "examples": [
            "98"
          ]
        },
        "region": {
          "type": ["string", "null"],
          "minLength": 1,
          "examples": [
            "Bydel Sagene"
          ]
        },
        "postnummer": {
          "type": "string",
          "minLength": 1,
          "examples": [
            "0175"
          ]
        },
        "poststed": {
          "type": "string",
          "minLength": 1,
          "examples": [
            "OSLO"
          ]
        },
        "landkode": {
          "$ref": "#/definitions/landkode"
        }
      }
    },
    "trygdeDekning": {
      "type": "string",
      "enum": ["UTEN_DEKNING", "FULL_DEKNING_EOSFO"]
    },
    "nullable-strukturertAdresse": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "gatenavn",
        "husnummer",
        "region",
        "postnummer",
        "poststed",
        "landkode"
      ],
      "properties": {
        "gatenavn": {
          "type": ["string", "null"],
          "minLength": 1,
          "examples": [
            "Waldemar Thranes gate"
          ]
        },
        "husnummer": {
          "type": ["string", "null"],
          "minLength": 1,
          "examples": [
            "98"
          ]
        },
        "region": {
          "type": ["string", "null"],
          "minLength": 1,
          "examples": [
            "Bydel Sagene"
          ]
        },
        "postnummer": {
          "type": ["string", "null"],
          "minLength": 1,
          "examples": [
            "0175"
          ]
        },
        "poststed": {
          "type": ["string", "null"],
          "minLength": 1,
          "examples": [
            "OSLO"
          ]
        },
        "landkode": {
          "$ref": "#/definitions/nullable-landkode"
        }
      }
    },
    "url": {
      "type": "string",
      "pattern": "(http(s)?:\\/\\/.)?(www\\.)?[-a-zA-Z0-9@:%._\\+~#=]{2,256}\\.[a-z]{2,6}\\b([-a-zA-Z0-9@:%_\\+.~#?&//=]*)",
      "examples": [
        "http://rina-url.local/"
      ]
    },
    "ustrukturertAdresse": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "landkode"
      ],
      "properties": {
        "adresselinjer": {
          "type": "array",
          "additionalItems": false,
          "maxItems": 5,
          "items": {
            "type": "string"
          }
        },
        "landkode": {
          "$ref": "#/definitions/landkode"
        }
      }
    }
  }
}
