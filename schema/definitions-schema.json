{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://melosys.nav.no/schemas/definitions-schema.json",
  "title": "The Definitions Schema",
  "definitions": {
    "nullable-aktoerID": {
      "type": ["string", "null"]
    },
    "aarmaaned": {
      "type": "string",
      "pattern": "^\\d{4}-\\d{2}$",
      "examples": [
        "2016-12"
      ]
    },
    "adresse": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "adressetype"
      ],
      "oneOf": [
        {
          "properties": {
            "adressetype": {"const": "STRUKTURERT"}
          },
          "required": ["strukturertAdresse"]
        },
        {
          "properties": {
            "adressetype": {"const": "USTRUKTURERT"}
          },
          "required": ["ustrukturertAdresse"]
        }
      ],
      "properties": {
        "adressetype": {
          "type": "string",
          "enum": ["STRUKTURERT", "USTRUKTURERT"]
        },
        "strukturertAdresse": {
          "$ref": "#/definitions/strukturertAdresse"
        },
        "ustrukturertAdresse": {
          "$ref": "#/definitions/ustrukturertAdresse"
        }
      }
    },
    "behandling": {
      "$id": "#/definitions/behandling",
      "type": "object",
      "title": "The Behandling Schema",
      "description": "An explanation about the purpose of this instance.",
      "additionalProperties": false,
      "required": [
        "behandlingID",
        "behandlingstype",
        "behandlingsstatus",
        "sisteOpplysningerHentetDato",
        "erUnderOppdatering",
        "registrertDato"
      ],
      "properties": {
        "behandlingID": {
          "$id": "#/definitions/behandling/properties/behandlingID",
          "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/behandlingID"
        },
        "behandlingstype": {
          "$id": "#/definitions/behandling/properties/behandlingstype",
          "title": "The behandlingstype Schema",
          "description": "An explanation about the purpose of this instance.",
          "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/kodeverk"
        },
        "behandlingsstatus": {
          "$id": "#/definitions/behandling/properties/behandlingsstatus",
          "title": "The behandlingsstatus Schema",
          "description": "An explanation about the purpose of this instance.",
          "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/kodeverk"
        },
        "sisteOpplysningerHentetDato":{
          "title": "The sisteOpplysningerHentetDato Schema",
          "$id": "#/definitions/behandling/properties/sisteOpplysningerHentetDato",
          "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/dato-tid"
        },
        "erUnderOppdatering": {
          "$id": "#/definitions/behandling/properties/erUnderOppdatering",
          "type": "boolean",
          "title": "The erUnderOppdatering Schema",
          "description": "An explanation about the purpose of this instance.",
          "default": "",
          "examples": [
            false,
            true
          ]
        },
        "registrertDato": {
          "$id": "#/definitions/behandling/properties/registrertDato",
          "title": "The RegistrertDato Schema",
          "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/dato-tid"
        }
      }
    },
    "behandlingID": {
      "type": "integer",
      "title": "The BehandlingID Schema",
      "default": 0,
      "examples": [
        1000023948
      ]
    },
    "databaseID": {
      "type": "integer",
      "title": "The DatabaseID Schema",
      "examples": [
        1234
      ]
    },
    "nullable-databaseID": {
      "type": ["integer", "null"],
      "title": "The Nullable-databaseID Schema",
      "examples": [
        1234,
        null
      ]
    },
    "dato":  {
      "type": "string",
      "format": "date",
      "examples": ["2018-08-27"]
    },
    "nullable-dato":  {
      "type": ["string","null"],
      "format": "date",
      "examples": ["2018-08-27"]
    },
    "dato-tid": {
      "type": "string",
      "format": "date-time",
      "examples": [
        "2018-08-27T07:16:03.273Z"
      ]
    },
    "nullable-dato-tid": {
      "type": ["string","null"],
      "format": "date-time",
      "examples": [
        "2018-08-27T07:16:03.273Z", null
      ]
    },
    "dokument":  {
      "type": "object",
      "title": "Dokument type definisjon",
      "additionalProperties": false,
      "required": [
        "dokumentID",
        "tittel"
      ],
      "properties": {
        "dokumentID": {"$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/dokumentID"
        },
        "tittel": {
          "type": ["string", "null"],
          "title": "Tittel Schema",
          "examples": ["Tittelen her"]
        }
      }
    },
    "nullable-dokument":  {
      "type": "object",
      "title": "Nullable Dokument type definisjon",
      "additionalProperties": false,
      "required": [
        "dokumentID",
        "tittel"
      ],
      "properties": {
        "dokumentID": {
          "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/nullable-dokumentID"
        },
        "tittel": {
          "type": "string",
          "title": "Tittel Schema",
          "examples": ["Tittelen her"]
        }
      }
    },
    "dokumentID": {
      "$id": "#/properties/dokumentID",
      "type": ["string"],
      "title": "The DokumentID Schema",
      "default": "",
      "examples": [
        "DOK_ID_4"
      ]
    },
    "nullable-dokumentID": {
      "$id": "#/properties/dokumentID",
      "type": ["string","null"],
      "title": "The Nullable DokumentID Schema",
      "default": "",
      "examples": [
        "DOK_ID_4", null
      ]
    },
    "fnr": {
      "type": "string",
      "default": "",
      "examples": [
        "81007251212"
      ],
      "pattern": "^\\d{11}$"
    },
    "nullable-fnr": {
      "type": ["string", "null"],
      "default": "",
      "examples": [
        "81007251212"
      ],
      "pattern": "^\\d{11}$"
    },
    "journalpostID": {
      "$id": "#/properties/journalpostID",
      "type": ["string"],
      "title": "The JournalpostID Schema",
      "default": "",
      "examples": [
        "DOK_321"
      ]
    },
    "kodeverk": {
      "type": "object",
      "title": "The Kodeverk Schema",
      "additionalProperties": false,
      "required": [
        "kode",
        "term"
      ],
      "properties": {
        "kode": {
          "type": "string",
          "title": "The Kode Schema",
          "default": "",
          "examples": [
            "OPPRETTET"
          ],
          "pattern": "^(.*)$"
        },
        "term": {
          "type": "string",
          "title": "The Term Schema",
          "default": "",
          "examples": [
            "Opprettet"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "landKode": {
      "type": "string",
      "minLength": 2,
      "examples": [
        "NO"
      ]
    },
    "orgnr": {
      "type": "string",
      "default": "",
      "examples": [
        "810072512"
      ],
      "pattern": "^\\d{9}$"
    },
    "nullable-orgnr": {
      "type": ["string", "null"],
      "default": "",
      "examples": [
        "810072512"
      ],
      "pattern": "^\\d{9}$|^$"
    },
    "periodeID": {
      "type": "integer",
      "title": "The PeriodeID Schema",
      "default": 0,
      "examples": [
        400110740
      ]
    },
    "periode": {
      "title": "Periode med fom tom",
      "type": "object",
      "required": [
        "fom",
        "tom"
      ],
      "additionalProperties": false,
      "properties": {
        "fom": {
          "type": ["string"],
          "format": "date",
          "examples": ["2014-03-12"]
        },
        "tom": {
          "type": ["string","null"],
          "format": "date",
          "examples": ["2014-03-12", null]
        }
      }
    },
    "nullable-periode": {
      "title": "Periode med fom tom",
      "type": "object",
      "required": [
        "fom",
        "tom"
      ],
      "additionalProperties": false,
      "properties": {
        "fom": {
          "type": ["string", "null"],
          "format": "date",
          "examples": ["2014-03-12"]
        },
        "tom": {
          "type": ["string","null"],
          "format": "date",
          "examples": ["2014-03-12", null]
        }
      }
    },
    "enum-representerer": {
      "type": "string",
      "enum": ["BRUKER", "ARBEIDSGIVER", "BEGGE"]
    },
    "representererKode": {
      "title": "The Representerer Schema",
      "default": null,
      "examples": ["BRUKER", "ARBEIDSGIVER", "BEGGE", null],
      "oneOf": [
        { "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/enum-representerer"},
        { "type": ["null"] }
      ]
    },
    "registerAdresse": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "gateadresse",
        "postnr",
        "poststed",
        "land"
      ],
      "properties": {
        "gateadresse": {
          "$id": "#/properties/registerAdresse/properties/gateadresse",
          "type": "object",
          "title": "The Gateadresse Schema",
          "additionalProperties": false,
          "required": [
            "gatenummer",
            "husbokstav",
            "husnummer",
            "gatenavn"
          ],
          "properties": {
            "gatenummer": {
              "$id": "#/properties/registerAdresse/properties/gateadresse/properties/gatenummer",
              "type": [
                "integer",
                "null"
              ],
              "title": "The Gatenummer Schema",
              "default": null,
              "examples": [
                null
              ]
            },
            "husbokstav": {
              "$id": "#/properties/registerAdresse/properties/gateadresse/properties/husbokstav",
              "type": [
                "string",
                "null"
              ],
              "title": "The Husbokstav Schema",
              "default": null,
              "examples": [
                null
              ]
            },
            "husnummer": {
              "$id": "#/properties/registerAdresse/properties/gateadresse/properties/husnummer",
              "type": [
                "integer",
                "null"
              ],
              "title": "The Husnummer Schema",
              "default": null,
              "examples": [
                null
              ]
            },
            "gatenavn": {
              "$id": "#/properties/registerAdresse/properties/gateadresse/properties/gatenavn",
              "type": [
                "string",
                "null"
              ],
              "title": "The Gatenavn Schema",
              "default": "",
              "examples": [
                "Olav Kyrres gate 9"
              ],
              "pattern": "^(.*)$"
            }
          }
        },
        "postnr": {
          "$id": "#/properties/registerAdresse/properties/postnr",
          "type": [
            "string",
            "null"
          ],
          "title": "The Postnr Schema",
          "default": "",
          "examples": [
            "3936"
          ],
          "pattern": "^(.*)$"
        },
        "poststed": {
          "$id": "#/properties/registerAdresse/properties/poststed",
          "type": [
            "string",
            "null"
          ],
          "title": "The Poststed Schema",
          "default": "",
          "examples": [
            "PORSGRUNN"
          ],
          "pattern": "^(.*)$"
        },
        "land": {
          "$id": "#/properties/registerAdresse/properties/land",
          "type": [
            "string",
            "object",
            "null"
          ],
          "title": "The Land Schema",
          "default": "",
          "examples": [
            "NORGE"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "saksbehandlingitem": {
      "$id": "#/definitions/saksbehandlingitem",
      "type": "object",
      "title": "The Saksbehandlingitem Schema",
      "description": "An explanation about the purpose of this instance.",
      "additionalProperties": false,
      "required": [
        "oppgaveID",
        "sammensattNavn",
        "fnr",
        "saksnummer",
        "sakstype",
        "behandling",
        "aktivTil",
        "soknadsperiode",
        "prioritet",
        "land",
        "versjon",
        "ansvarligID"
      ],
      "properties": {
        "oppgaveID": {
          "$id": "#/definitions/saksbehandlingitem/properties/oppgaveID",
          "type": "string",
          "title": "The Oppgaveid Schema",
          "default": "",
          "examples": [
            "174562068"
          ],
          "pattern": "^(.*)$"
        },
        "sammensattNavn": {
          "$id": "#/definitions/saksbehandlingitem/properties/sammensattNavn",
          "type": "string",
          "title": "The Sammensattnavn Schema",
          "default": "",
          "examples": [
            "GLITRENDE HATT"
          ],
          "pattern": "^(.*)$"
        },
        "fnr": {
          "$id": "#/definitions/saksbehandlingitem/properties/fnr",
          "type": "string",
          "title": "The Fnr Schema",
          "default": "",
          "examples": [
            "19117220349"
          ],
          "pattern": "^(.*)$"
        },
        "saksnummer": {
          "$id": "#/definitions/saksbehandlingitem/properties/saksnummer",
          "type": "string",
          "title": "The Saksnummer Schema",
          "default": "",
          "examples": [
            "4"
          ],
          "pattern": "^(.*)$"
        },
        "sakstype": {
          "$id": "#/definitions/saksbehandlingitem/properties/sakstype",
          "title": "The Sakstypekode Schema",
          "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/kodeverk"
        },
        "behandling": {
          "$id": "#/definitions/saksbehandlingitem/properties/behandling",
          "title": "The Behandling Schema",
          "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/behandling"
        },
        "aktivTil": {
          "$id": "#/definitions/saksbehandlingitem/properties/aktivTil",
          "type": "string",
          "format": "date",
          "title": "The Aktivtil Schema",
          "default": "",
          "examples": [
            "2018-01-28"
          ]
        },
        "soknadsperiode": {
          "title": "The Soknadsperiode Schema",
          "$id": "#/definitions/saksbehandlingitem/properties/soknadsperiode",
          "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/nullable-periode"
        },
        "prioritet": {
          "$id": "#/definitions/saksbehandlingitem/properties/prioritet",
          "type": "string",
          "title": "The Prioritet Schema",
          "default": "",
          "examples": [
            "HOY"
          ],
          "pattern": "^(.*)$"
        },
        "land": {
          "$id": "#/definitions/saksbehandlingitem/properties/land",
          "type": "array",
          "title": "The Land Schema",
          "uniqueItems": true,
          "additionalItems": false,
          "items": {
            "$id": "",
            "type": "string",
            "title": "The LandKode item Schema",
            "default": "",
            "examples": [
              "NO"
            ],
            "pattern": "^(.*)$"
          }
        },
        "versjon": {
          "$id": "#/definitions/saksbehandlingitem/properties/versjon",
          "type": "integer",
          "title": "The Versjon Schema",
          "default": 0,
          "examples": [
            1
          ]
        },
        "ansvarligID": {
          "$id": "#/definitions/saksbehandlingitem/properties/ansvarligID",
          "type": "string",
          "title": "The Ansvarligid Schema",
          "default": "",
          "examples": [
            "Z991001"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "strukturertAdresse": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "gatenavn",
        "husnummer",
        "region",
        "postnummer",
        "poststed",
        "landKode"
      ],
      "properties": {
        "gatenavn": {
          "type": "string",
          "minLength": 1,
          "examples": [
            "Waldemar Thranes gate"
          ]
        },
        "husnummer": {
          "type": ["string", "null"],
          "minLength": 1,
          "examples": [
            "98"
          ]
        },
        "region": {
          "type": ["string", "null"],
          "minLength": 1,
          "examples": [
            "Bydel Sagene"
          ]
        },
        "postnummer": {
          "type": "string",
          "minLength": 1,
          "examples": [
            "0175"
          ]
        },
        "poststed": {
          "type": "string",
          "minLength": 1,
          "examples": [
            "OSLO"
          ]
        },
        "landKode": {
          "$ref": "#/definitions/landKode"
        }
      }
    },
    "ustrukturertAdresse": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "landKode"
      ],
      "properties": {
        "adresselinjer": {
          "type": "array",
          "additionalItems": false,
          "maxItems": 5,
          "items": {
            "type": "string"
          }
        },
        "landKode": {
          "$ref": "#/definitions/landKode"
        }
      }
    }
  }
}
