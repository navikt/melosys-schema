{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "./avklartefakta-oppsummering-schema.json",
  "title": "The Avklartefakta Oppsummering Schema",
  "type": "object",
  "additionalProperties": true,
  "required": [
    "virksomheter",
    "medfolgendeFamilie"
  ],
  "properties": {
    "virksomheter" : {
      "$id": "#/properties/virksomheter",
      "virksomhetIDer" : {
        "$ref": "./definitions-schema.json#/definitions/virksomhetIDer"
      }
    },
    "medfolgendeFamilie": {
      "$id": "#/properties/medfolgendeFamilie",
      "type": "array",
      "uniqueItems": true,
      "items": {
        "$ref": "./definitions-schema.json#/definitions/medfolgendeFamilieObjekt"
      },
      "default": []
    }
  }
}
