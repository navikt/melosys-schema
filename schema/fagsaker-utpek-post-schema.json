{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "./fagsaker-utpek-post-schema.json",
  "type": "object",
  "title": "The Fagsaker utpek post schema",
  "additionalProperties": false,
  "required": [
    "mottakerinstitusjoner",
    "fritekstSed",
    "fritekstBrev"
  ],
  "properties": {
    "mottakerinstitusjoner": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "fritekstSed": {
      "type": ["string", "null"],
      "pattern": "^(.*)$"
    },
    "fritekstBrev": {
      "type": ["string", "null"]
    }
  }
}
