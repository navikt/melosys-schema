{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "./oppgaver-sok-schema.json",
  "title": "The oppgaver-sok-get Schema",
  "type": "array",
  "uniqueItems": true,
  "items": {
    "$ref": "#/definitions/oppgave"
  },
  "definitions": {
    "oppgave": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "oppgaveID",
        "tema",
        "oppgavetype",
        "registrertDato",
        "frist",
        "sakID",
        "journalpostID"
      ],
      "properties": {
        "journalpostID": {
          "$ref": "./definitions-schema.json#/definitions/journalpostID"
        },
        "oppgaveID": {
          "$ref": "./definitions-schema.json#/definitions/oppgaveID"
        },
        "tema": {
          "type": "string",
          "examples": [
            "MED",
            "UFM"
          ]
        },
        "oppgavetype": {
          "type": "string",
          "examples": [
            "Behandle sak (Manuell)",
            "Behandle SED"
          ]
        },
        "registrertDato": {
          "$ref": "./definitions-schema.json#/definitions/dato-tid"
        },
        "frist": {
          "$ref": "./definitions-schema.json#/definitions/dato"
        },
        "sakID": {
          "type": ["string", "null"]
        }
      }
    }
  }
}
