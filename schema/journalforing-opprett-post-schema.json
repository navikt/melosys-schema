{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://melosys.nav.no/schemas/journalforing-opprett-schema.json",
  "type": "object",
  "title": "The Root Schema",
  "additionalProperties": false,
  "required": [
    "avsenderType",
    "journalpostID",
    "oppgaveID",
    "behandlingstypeKode",
    "brukerID",
    "avsenderID",
    "avsenderNavn",
    "arbeidsgiverID",
    "representantID",
    "representantKontaktPerson",
    "representererKode",
    "hoveddokument",
    "vedlegg",
    "fagsak",
    "anmodningOmUnntak",
    "skalTilordnes",
    "ikkeSendForvaltingsmelding",
    "mottattDato"
  ],
  "properties": {
    "avsenderType": {
      "type": "string"
    },
    "journalpostID": {
      "$id": "#/properties/journalpostID",
      "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/journalpostID"
    },
    "oppgaveID": {
      "title": "The Oppgaveid Schema",
      "$id": "#/properties/oppgaveID",
      "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/oppgaveID"
    },

    "behandlingstypeKode": {
      "title": "The Behandlingsype Schema",
      "$id": "#/properties/behandlingstypeKode",
      "type": "string",
      "default": "",
      "pattern": "^(.*)$"
    },
    "brukerID": {
      "$id": "#/properties/brukerID",
      "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/fnr"
    },
    "avsenderID": {
      "$id": "#/properties/avsenderID",
      "type": "string",
      "title": "The Avsenderid Schema",
      "default": "",
      "examples": [
        "30098000492"
      ],
      "pattern": "^(.*)$"
    },
    "avsenderNavn": {
      "$id": "#/properties/avsenderNavn",
      "type": "string",
      "title": "The Avsendernavn Schema",
      "default": "",
      "examples": [
        "PEN FUL"
      ],
      "pattern": "^(.*)$"
    },
    "arbeidsgiverID": {
      "$id": "#/properties/arbeidsgiverID",
      "type": ["string","null"],
      "title": "The Arbeidsgiverid Schema",
      "default": "",
      "examples": [
        "171178022"
      ],
      "pattern": "^[0-9]{0,9}$"
    },
    "representantID": {
      "$id": "#/properties/representantID",
      "type": ["string","null"],
      "title": "The Representantid Schema",
      "default": "",
      "examples": [
        "300980004"
      ],
      "pattern": "^[0-9]{0,9}$"
    },
    "representantKontaktPerson": {
      "$id": "#/properties/representantKontaktPerson",
      "type": ["string","null"],
      "title": "The RepresentantKontaktPerson Schema",
      "default": ""
    },
    "representererKode": {
      "title": "The RepresentererKode Schema",
      "$id": "#/properties/representererKode",
      "type": "string",
      "default": "",
      "pattern": "^(.*)$"
    },
    "hoveddokument": {
      "$id": "#/items/properties/hoveddokument",
      "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/dokument"
    },
    "vedlegg": {
      "$id": "#/items/properties/vedlegg",
      "type": "array",
      "title": "The Vedlegg Schema",
      "items": {
        "$id": "#/items/properties/vedlegg/items",
        "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/dokument"
      }
    },
    "fagsak": {
      "$id": "#/properties/fagsak",
      "type": "object",
      "title": "The Fagsak Schema",
      "additionalProperties": false,
      "required": [
        "sakstype",
        "soknadsperiode",
        "land"
      ],
      "properties": {
        "sakstype": {
          "$id": "#/properties/fagsak/properties/sakstype",
          "type": "string",
          "title": "The Sakstype Schema",
          "default": "",
          "examples": [
            "EU_EOS"
          ],
          "pattern": "^(.*)$"
        },
        "soknadsperiode": {
          "title": "The Soknadsperiode Schema",
          "$id": "#/properties/fagsak/properties/soknadsperiode",
          "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/nullable-periode"
        },
        "land": {
          "$id": "#/properties/fagsak/properties/land",
          "type": "array",
          "title": "The Land Schema",
          "uniqueItems": true,
          "items": {
            "$id": "#/properties/fagsak/properties/land/items",
            "type": "string",
            "title": "The Items Schema",
            "default": "",
            "examples": [
              "NO",
              "ES"
            ],
            "pattern": "^(.*)$"
          }
        }
      }
    },
    "anmodningOmUnntak": {
      "$id": "#/properties/anmodningOmUnntak",
      "type": "object",
      "title": "The Anmodningomunntak Schema",
      "required": [
        "unntakFraLovvalgsland",
        "lovvalgsbestemmelse",
        "unntakFraLovvalgsbestemmelse"
      ],
      "properties": {
        "unntakFraLovvalgsland": {
          "$id": "#/properties/anmodningOmUnntak/properties/unntakFraLovvalgsland",
          "type": [
            "string",
            "null"
          ],
          "title": "The UnntakFraLovvalgsland Schema",
          "default": "",
          "examples": [
            "NO"
          ],
          "pattern": "^(.*)$"
        },
        "lovvalgsbestemmelse": {
          "$id": "#/properties/anmodningOmUnntak/properties/lovvalgsbestemmelse",
          "type": [
            "string",
            "null"
          ],
          "title": "The Lovvalgsbestemmelse Schema",
          "default": "",
          "examples": [
            "FO_883_2004_ART16_1"
          ]
        },
        "unntakFraLovvalgsbestemmelse": {
          "$id": "#/properties/anmodningOmUnntak/properties/unntakFraLovvalgsbestemmelse",
          "type": [
            "string",
            "null"
          ],
          "title": "The UnntakFraLovvalgsbestemmelse Schema",
          "default": "",
          "examples": [
            "FO_883_2004_ART12_1"
          ]
        }
      }
    },
    "skalTilordnes": {
      "$id": "#/items/properties/skalTilordnes",
      "type": "boolean",
      "title": "The SkalTilordnes Schema",
      "default": false,
      "examples": [
        true
      ]
    },
    "ikkeSendForvaltingsmelding": {
      "$id": "#/items/properties/ikkeSendForvaltingsmelding",
      "type": "boolean",
      "title": "The IkkeSendForvaltingsmelding Schema",
      "default": false,
      "examples": [
        true
      ]
    },
    "mottattDato": {
      "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/dato",
    }
  }
}
