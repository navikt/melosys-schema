{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://melosys.nav.no/schemas/dokumenter-utkast-pdf-post-schema.json",
  "type": [
    "object",
    "null"
  ],
  "title": "The Root Schema",
  "additionalProperties": false,
  "required": [
    "mottaker",
    "begrunnelseKode",
    "fritekst",
    "ytterligereInformasjon"
  ],
  "properties": {
    "mottaker": {
      "$id": "#/properties/mottaker",
      "type": [
        "string",
        "null"
      ],
      "title": "The Mottaker Schema",
      "description": "",
      "default": "",
      "examples": [
        "ARBEIDSGIVER",
        "BRUKER",
        "REPRESENTANT",
        "MYNDIGHET"
      ],
      "enum": ["ARBEIDSGIVER","BRUKER","REPRESENTANT","MYNDIGHET"],
      "pattern": "^(.*)$"
    },
    "fritekst": {
      "$id": "#/properties/fritekst",
      "type": [
        "string",
        "null"
      ],
      "title": "The Fritekst Schema",
      "default": "",
      "examples": ["Fritekst"],
      "pattern": "^(.*)$"
    },
    "begrunnelseKode": {
      "$id": "#/properties/begrunnelseKode",
      "type": [
        "string",
        "null"
      ],
      "title": "The BegrunnelseKode Schema",
      "default": "",
      "examples": [
        "ANMODNING_UNNTAK",
        "AVSLAG_ARBEIDSGIVER",
        "AVSLAG_YRKESAKTIV",
        "ATTEST_A1",
        "INNVILGELSE_ARBEIDSGIVER",
        "INNVILGELSE_YRKESAKTIV",
        "MELDING_FORVENTET_SAKSBEHANDLINGSTID",
        "MELDING_MANGLENDE_OPPLYSNINGER",
        "MELDING_HENLAGT_SAK",
        "ORIENTERING_ANMODNING_UNNTAK"
      ],
      "pattern": "^(.*)$"
    },
    "ytterligereInformasjon": {
      "type": [
        "string",
        "null"
      ]
    }
  }
}
