{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "./person-schema.json",
  "type": "object",
  "title": "The Root Schema",
  "additionalProperties": false,
  "required": [
    "fnr",
    "sivilstand",
    "statsborgerskap",
    "sammensattNavn",
    "personstatus",
    "kjoenn",
    "foedselsdato",
    "familiemedlemmer"
  ],
  "properties": {
    "fnr": {
      "title": "The Fnr Schema",
      "$id": "#/properties/fnr",
      "$ref": "./definitions-schema.json#/definitions/fnr"
    },
    "sivilstand": {
      "$id": "#/properties/sivilstand",
      "title": "The Sivilstand Schema",
      "$ref": "./definitions-schema.json#/definitions/kodeverk"
    },
    "statsborgerskap": {
      "$id": "#/properties/statsborgerskap",
      "title": "The Statsborgerskap Schema",
      "$ref": "./definitions-schema.json#/definitions/kodeverk"
    },
    "sammensattNavn": {
      "$id": "#/properties/sammensattNavn",
      "type": "string",
      "title": "The Sammensattnavn Schema",
      "default": "",
      "examples": [
        "HATT GLITRENDE"
      ],
      "pattern": "^(.*)$"
    },
    "personstatus": {
      "$id": "#/properties/personstatus",
      "title": "The Personstatus Schema",
      "$ref": "./definitions-schema.json#/definitions/kodeverk"
    },
    "kjoenn": {
      "$id": "#/properties/kjoenn",
      "title": "The Kjoenn Schema",
      "$ref": "./definitions-schema.json#/definitions/kodeverk"
    },
    "foedselsdato": {
      "title": "The Foedselsdato Schema",
      "$id": "#/properties/foedselsdato",
      "$ref": "./definitions-schema.json#/definitions/dato"
    },
    "bostedsadresse": {
      "$id": "#/properties/postadresse",
      "$ref": "./definitions-schema.json#/definitions/registerAdresse"
    },
    "postadresse": {
      "$id": "#/properties/postadresse",
      "$ref": "./definitions-schema.json#/definitions/ustrukturertAdresse"
    },
    "midlertidigPostadresse": {
      "$id": "#/properties/midlertidigPostadresse",
      "$ref": "./definitions-schema.json#/definitions/adresse"
    },
    "familiemedlemmer": {
      "$id": "#/properties/familiemedlemmer",
      "$ref": "./definitions-schema.json#/definitions/familiemedlemmer"
    }
  }
}
