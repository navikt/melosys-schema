{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://melosys.nav.no/schemas/kontaktopplysninger-schema.json",
  "type": ["object", "array"],
  "title": "The Root Schema",
  "additionalProperties": false,
  "required": [
    "kontaktnavn",
    "kontaktorgnr",
    "kontakttlf"
  ],
  "properties": {
    "kontaktnavn": {
      "$id": "#/properties/kontaktnavn",
      "type": "string",
      "title": "The Kontaktnavn Schema",
      "default": "",
      "examples": [
        "GLITRENDE HATT"
      ],
      "pattern": "^(.*)$"
    },
    "kontaktorgnr": {
      "$id": "#/properties/kontaktorgnr",
      "title": "The Kontaktorgnr Schema",
      "$ref": "http://melosys.nav.no/schemas/definitions-schema.json#/definitions/nullable-orgnr"
    },
    "kontakttlf": {
      "$id": "#/properties/kontakttlf",
      "type": ["string", "null"],
      "title": "The Kontakttlf Schema",
      "default": "",
      "examples": [
        "910099035"
      ],
      "pattern": "^(.*)$"
    }
  }
}
